import java.util.*;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int fromBase, toBase;
		String number;
		
		System.out.println("From base: ");
		fromBase = in.nextInt();
		System.out.println("To base: ");
		toBase = in.nextInt();
		System.out.println("Number: ");
		number = in.nextLine();
		
		System.out.println("Answer: " + Integer.toString(Integer.parseInt(number, fromBase), toBase));
	}
	
}
